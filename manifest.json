{
    "manifest_version": 3,
    "name": "Cirno！",
    "version": "1.0",
    "description": "用来解决CNKI手动选择500个很累的严重问题",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  